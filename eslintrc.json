{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:import/warnings", "plugin:react/recommended"],
  "plugins": ["import", "react"],
  "rules": {
    "import/no-unresolved": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
